{
  "extends": ["eslint:recommended", "airbnb"],
  "rules": {
    "linebreak-style": [
      "error",
      "windows"
    ],
    "comma-dangle": ["error", {"functions": "never"}],
    "no-underscore-dangle": 0,
    "no-nested-ternary": 0,
    "radix": 0,
    "semi": 0,
    "react/prop-types": 0,
    "jsx-a11y/anchor-is-valid": 0
  },
  "env": {
    "es6": true,
    "node": true,
    "browser": true
  }
}
